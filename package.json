    "eslint-config-prettier": "^9.1.0",
    "i": "^0.3.7",
    "klona": "^2.0.6",
    "mitt": "^3.0.1",
    "npm": "^10.2.5",
    "prettier": "^3.2.5"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxt/module-builder": "^0.5.4",
    "@nuxt/schema": "^3.8.2",
    "@nuxt/test-utils": "^3.8.1",
    "@types/node": "^20.10.3",
    "changelogen": "^0.5.5",
    "eslint": "^8.55.0",
    "jsdom": "^23.0.1",
    "nuxt": "^3.8.2",
    "vitest": "^0.33.0"
  }
